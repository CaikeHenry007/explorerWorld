{
  "expo": {
    "name": "explorerWorld",
    "slug": "explorerWorld",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/images/EXPLORAR.INICIO.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/images/EXPLORAR.INICIO.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/images/EXPLORAR.INICIO.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/images/EXPLORAR.INICIO.png"
    },
    "scripts": {
      "postinstall": "npx jetify"
    },
    "plugins": [
      "expo-font"
    ]
  }
}